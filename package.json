{
  "name": "@digital-swing/lazy-module",
  "version": "2.0.0",
  "description": "Asynchroneously load modules when they are required",
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/bundle.min.js",
  "unpkg": "./dist/bundle.min.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/types"
  ],
  "sideEffects": false,
  "homepage": "https://github.com/digital-swing/lazy-module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/digital-swing/lazy-module.git",
  "author": "Lucas Demea <lucas@digital-swing.com>",
  "license": "MIT",
  "engines": {
    "node": ">=v14.18.0"
  },
  "scripts": {
    "build": "rimraf ./dist && rollup -c rollup.config.ts --configPlugin typescript",
    "dev": "rollup -c rollup.config.ts --configPlugin typescript -w",
    "test": "jest --passWithNoTests",
    "cov": "jest --coverage --passWithNoTests",
    "lint": "npm run lint:js && npm run lint:css",
    "docs:html": "typedoc --plugin none --readme none --out docs/types src/index.ts",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --readme none --out docs/types src/index.ts",
    "docs:dev": "vuepress dev docs",
    "docs:vue": "vuepress build docs",
    "lint:js": "eslint resources/scripts",
    "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
    "prepare": "is-ci || husky install"
  },
  "peerDependencies": {
    "intersection-observer": "^0.12.2",
    "requestidlecallback-polyfill": "^1.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.2",
    "@types/node": "18.17.1",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "esbuild": "0.18.17",
    "eslint": "8.45.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-typescript-sort-keys": "2.3.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "rimraf": "5.0.1",
    "rollup": "3.26.3",
    "rollup-plugin-dts": "5.3.0",
    "rollup-plugin-esbuild": "5.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typedoc": "0.24.8",
    "typedoc-plugin-markdown": "3.15.4",
    "typescript": "5.1.6"
  }
}

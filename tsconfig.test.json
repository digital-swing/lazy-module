{
  "extends": "./tsconfig.json",
  "include": [
    "src/**/__tests__/**/*.ts",
    "src/**/__mocks__/**/*.ts",
    "src/**/*.test.ts",
    "config.ts",
    "jest.config.ts"
  ],
  "exclude": [],
  "compilerOptions": {
    "plugins": [
      {
        "cacheBetweenTests": false
      }
    ],
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "types": [
      "vitest/globals"
    ]
  }
}